<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sucursal.Invent3Controller">
   <children>
      <VBox layoutX="14.0" layoutY="14.0" prefHeight="383.0" prefWidth="578.0">
         <children>
            <HBox alignment="BASELINE_CENTER" prefHeight="37.0" prefWidth="578.0">
               <children>
                  <Label text="INVENTARIO">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="34.0" prefWidth="578.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="92.0" text="Nombre: " />
                  <TextField fx:id="nombre" prefHeight="25.0" prefWidth="167.0" />
               </children>
            </HBox>
            <HBox prefHeight="33.0" prefWidth="578.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="92.0" text="Id: " />
                  <TextField fx:id="id" prefHeight="25.0" prefWidth="168.0" />
               </children>
            </HBox>
            <HBox prefHeight="33.0" prefWidth="578.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="92.0" text="Precio: " />
                  <TextField fx:id="precio" prefHeight="25.0" prefWidth="168.0" />
               </children>
            </HBox>
            <HBox prefHeight="32.0" prefWidth="578.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="93.0" text="cantidad: " />
                  <TextField fx:id="cantidad" prefHeight="25.0" prefWidth="168.0" />
               </children>
            </HBox>
            <TableView fx:id="table" prefHeight="140.0" prefWidth="578.0">
              <columns>
                <TableColumn fx:id="nombre_tab" prefWidth="75.0" text="Nombre" />
                <TableColumn fx:id="id_tab" prefWidth="75.0" text="ID" />
                  <TableColumn fx:id="precio_tab" prefWidth="75.0" text="Precio" />
                  <TableColumn fx:id="cantidad_tab" prefWidth="75.0" text="Cantidad" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <HBox alignment="CENTER" prefHeight="35.0" prefWidth="578.0" spacing="50.0">
               <children>
                  <Button fx:id="guardar" mnemonicParsing="false" onAction="#Guardar_producto" text="Guardar" />
                  <Button fx:id="mostrar" mnemonicParsing="false" onAction="#mostrar_productos" text="Mostrar" />
                  <Button fx:id="salir" mnemonicParsing="false" onAction="#Volver" text="Salir" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
