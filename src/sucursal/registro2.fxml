               </children>
            </HBox>
            <HBox prefHeight="46.0" prefWidth="503.0">
               <children>
                  <CheckBox fx:id="vendedor" mnemonicParsing="false" onAction="#checkEvent" prefHeight="17.0" prefWidth="121.0" text="Vendedor" />
