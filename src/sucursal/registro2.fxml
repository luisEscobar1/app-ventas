               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="64.0" prefWidth="503.0" spacing="50.0">
               <children>
                  <Button fx:id="guardar" mnemonicParsing="false" onAction="#guardar_registro" text="Guardar" />
                  <Button fx:id="Salir" mnemonicParsing="false" onAction="#Salir" text="Salir" />
               </children>
            </HBox>
         </children>
